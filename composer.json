{
    "name": "php-tuf/phpcodesniffer-standard",
    "type": "phpcodesniffer-standard",
    "description": "PHP_CodeSniffer rules (sniffs) for the PHP-TUF coding standard",
    "keywords": [
        "phpcodesniffer-standard",
        "phpcs",
        "standards"
    ],
    "homepage": "https://github.com/php-tuf/phpcodesniffer-standard",
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Carden",
            "email": "travis.carden@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "squizlabs/php_codesniffer": "^3.5"
    },
    "suggest": {
        "dealerdirect/phpcodesniffer-composer-installer": "Automatically install PHP_CodeSniffer coding standards (rulesets)."
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "PhpTuf\\CodingStandards\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/php-tuf/phpcodesniffer-standard/issues",
        "source": "https://github.com/php-tuf/phpcodesniffer-standard"
    }
}
